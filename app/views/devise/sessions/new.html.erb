<% title t(:login) %>  
  
<%= form_for(resource,:as=>resource_name, :url => session_path(resource_name)) do |f| %>  
  <ol class="formList">  
    <li><%= f.label t(:email) %> <%= f.text_field :email %></li>  
    <li><%= f.label t(:password) %> <%= f.password_field :password %></li>  
    <% if devise_mapping.rememberable? -%>  
    <li><%= f.check_box :remember_me %> <%= f.label t(:remember_me) %></li>  
    <% end %>  
    <li><%= f.submit t(:login) %></li>  
  </ol>  
<% end %>  
<%= render :partial => "devise/shared/links" %>